      "source": [
        "def fibonacci(n):\n",
        "    if(n == 0):\n",
        "        return 0 \n",
        "    elif(n == 1 or n == 2):\n",
        "        return 1\n",
        "    else:\n",
        "        return(fibonacci(n-1)+fibonacci(n-2))\n",
        "n = int(input())\n",
        "print(fibonacci(n))"
      ],
