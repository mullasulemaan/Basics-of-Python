        "#!/bin/python3\n",
        "import sys\n",
        "n = int(input())\n",
        "if(n % 2 == 1):\n",
        "    print(\"Weird\")\n",
        "elif(n % 2 == 0 and 2  <= n <= 5):\n",
        "    print(\"Not Weird\")\n",
        "elif(n % 2 == 0 and 6 <= n <= 20):\n",
        "    print(\"Weird\")\n",
        "else:\n",
        "    print(\"Not Weird\")"
